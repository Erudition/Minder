{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "pretty": true,
    "removeComments": true,
    "experimentalDecorators": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "noLib": false,
    "importHelpers": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2015.iterable"
    ],
    "types": [
      "jasmine"
    ],
    "baseUrl": ".",
    "paths": {
      "*": [
        "./node_modules/tns-core-modules/*",
        "./node_modules/*"
      ]
    }
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "test",
    "platforms"
  ]
}
